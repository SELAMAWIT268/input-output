# 1. Even or odd
n=7
print("Even" if n%2==0 else "Odd")

# 2. Largest of 3
a,b,c=5,9,3
if a>=b and a>=c: print(a)
elif b>=a and b>=c: print(b)
else: print(c)

# 3. Age category
age=18
if age<13: print("Child")
elif age<=19: print("Teen")
else: print("Adult")

# 4. Triangle type
a,b,c=3,4,5
if a==b==c: print("Equilateral")
elif a==b or b==c or a==c: print("Isosceles")
else: print("Scalene")


